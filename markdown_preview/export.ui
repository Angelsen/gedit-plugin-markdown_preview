<?xml version="1.0" encoding="UTF-8"?>
<interface domain="gedit-plugin-markdown-preview">
	<object class="GtkBox" id="content-area">
		<property name="visible">True</property>
		<property name="orientation">vertical</property>
		<property name="height-request">300</property>
		<property name="spacing">20</property>
		<property name="margin">20</property>
		<child>
			<object class="GtkBox">
				<property name="visible">True</property>
				<property name="spacing">20</property>
				<child>
					<object class="GtkLabel">
						<property name="label" translatable="yes">Output file format:</property>
						<property name="visible">True</property>
					</object>
					<packing>
						<property name="pack-type">start</property>
					</packing>
				</child>
				<child>
					<object class="GtkStackSwitcher">
						<property name="stack">export_stack</property>
						<property name="visible">True</property>
					</object>
					<packing>
						<property name="pack-type">end</property>
					</packing>
				</child>
			</object>
		</child>
		<child>
			<object class="GtkStack" id="export_stack">
				<property name="visible">True</property>
				<property name="expand">True</property>
				<child>
					<object class="GtkBox">
						<property name="visible">True</property>
						<property name="orientation">vertical</property>
						<child>
							<object class="GtkRadioButton">
								<property name="visible">True</property>
								<child>
									<object class="GtkLabel">
										<property name="label">export_stack_html</property>
										<property name="visible">True</property>
									</object>
								</child>
							</object>
						</child>
						<child>
							<object class="GtkRadioButton">
								<property name="visible">True</property>
								<child>
									<object class="GtkLabel">
										<property name="label">export_stack_html</property>
										<property name="visible">True</property>
									</object>
								</child>
							</object>
						</child>
					</object>
					<packing>
						<property name="title">HTML</property>
						<property name="name">export_html</property>
					</packing>
				</child>
				<child>
					<object class="GtkBox">
						<property name="visible">True</property>
						<property name="orientation">vertical</property>
						<child>
							<object class="GtkRadioButton">
								<property name="visible">True</property>
								<child>
									<object class="GtkLabel">
										<property name="label">export_stack_pdf</property>
										<property name="visible">True</property>
									</object>
								</child>
							</object>
						</child>
						<child>
							<object class="GtkRadioButton">
								<property name="visible">True</property>
								<child>
									<object class="GtkLabel">
										<property name="label">export_stack_pdf</property>
										<property name="visible">True</property>
									</object>
								</child>
							</object>
						</child>
					</object>
					<packing>
						<property name="title">PDF</property>
						<property name="name">export_pdf</property>
					</packing>
				</child>
				<child>
					<object class="GtkBox">
						<property name="visible">True</property>
						<property name="orientation">vertical</property>
						<child>
							<object class="GtkRadioButton">
								<property name="visible">True</property>
								<child>
									<object class="GtkLabel">
										<property name="label">export_stack_custom</property>
										<property name="visible">True</property>
									</object>
								</child>
							</object>
						</child>
						<child>
							<object class="GtkRadioButton">
								<property name="visible">True</property>
								<child>
									<object class="GtkLabel">
										<property name="label">export_stack_custom</property>
										<property name="visible">True</property>
									</object>
								</child>
							</object>
						</child>
					</object>
					<packing>
						<property name="title">Custom</property>
						<property name="name">export_custom</property>
					</packing>
				</child>
			</object>
		</child>
	</object>
</interface>

